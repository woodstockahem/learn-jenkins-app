{
  "requiresCompatibilities": ["FARGATE"],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "learnjenkinsapp",
      "image": "604076720022.dkr.ecr.eu-north-1.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
      "portMappings": [
        {
          "name": "nginx-80-tip",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::604076720022:role/ecsTaskExecutionRole"
}
